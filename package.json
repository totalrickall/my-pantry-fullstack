{
  "name": "in-class-db-api",
  "version": "1.0.0",
  "description": "",
  "main": "server/lib/index.js",
  "scripts": {
    "build:client": "webpack",
    "build:server": "babel ./server/src -d ./server/lib",
    "clean": "npm run clean:client && npm run clean:server",
    "clean:client": "rimraf ./client/dist",
    "clean:server": "rimraf ./server/lib",
    "dev": "source .ENV && npm run clean && npm run build:server && concurrently -r -k \"npm run watch:client\" \"npm run watch:server\" \"nodemon --watch server/lib --delay 1\"",
    "start": "npm run clean && npm run build:client && npm run build:server && npm run start:server",
    "start:server": "node server/lib/index.js",
    "watch:client": "webpack --watch",
    "watch:server": "babel ./server/src -w -d ./server/lib"
  },
  "author": "Covalence",
  "license": "ISC",
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^1.0.0",
    "email-validator": "^2.0.4",
    "es6-promise": "^4.2.4",
    "express": "^4.16.3",
    "file-loader": "^1.1.11",
    "isomorphic-fetch": "^2.2.1",
    "lodash": "^4.17.10",
    "moment": "^2.22.2",
    "moment-duration-format": "^2.2.2",
    "morgan": "^1.9.0",
    "mysql": "^2.15.0",
    "passport": "^0.4.0",
    "passport-http-bearer": "^1.0.1",
    "passport-local": "^1.0.0",
    "password-validator": "^4.1.1",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-modal": "^3.5.1",
    "react-router-dom": "^4.2.2",
    "rimraf": "^2.6.2",
    "style-loader": "^0.21.0",
    "uuid": "^3.3.2",
    "webpack": "^4.16.4",
    "webpack-cli": "^3.1.0"
  },
  "devDependencies": {
    "concurrently": "^3.5.1",
    "nodemon": "^1.18.3"
  }
}
